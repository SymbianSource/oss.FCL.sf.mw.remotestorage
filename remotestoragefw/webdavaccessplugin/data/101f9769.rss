// INCLUDES
#include "registryinfo.rh"

//  RESOURCE DEFINITIONS 
// -----------------------------------------------------------------------------
//   
// REGISTRY_INFO theInfo
// Declares info for the "remoteaccess/http" implementation
//
// -----------------------------------------------------------------------------
//
RESOURCE REGISTRY_INFO theInfo
	{
	// UID for the DLL
	dll_uid = 0x101F9769;
	// Declare array of interface info
	interfaces = 
		{
		INTERFACE_INFO
			{
			// UID of interface that is implemented
			interface_uid = 0x101F96E3;
			implementations = 
				{
				// Info for CDavAccess
				IMPLEMENTATION_INFO
					{
					implementation_uid = 0x101F9769;
					version_no = 1;
					display_name = "http";
					default_data = "remoteaccess/http||WebDAV";
					opaque_data = "params";
					}
				};
			}
		};
	}

// End of File
